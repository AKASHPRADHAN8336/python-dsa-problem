class Solution(object):
    def findMaxConsecutiveOnes(self, nums):
        cnt=0
        max_count=0
        n=len(nums)

        for i in range(n):
            if nums[i]==1:
                cnt+=1
                if max_count<cnt:
                    max_count=cnt
                # max_count=max(cnt,max_count)
            else: cnt=0

        return max_count

            
        
