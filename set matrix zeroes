class Solution(object):
    def setZeroes(self, matrix):
        for row in range(len(matrix)):
            if matrix[row][0] == 0:
                zeroinFirstCol = True
                for col in range(1, len(matrix[0])):
                    if matrix[row][col] == 0:
                        matrix[row][0] = 0
                        matrix[0][col] = 0
}
        
