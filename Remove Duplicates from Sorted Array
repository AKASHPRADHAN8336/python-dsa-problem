class Solution(object):
    def removeDuplicates(self, nums):
        n=len(nums)
        i=1
        j=1
        if n==1:
            return 1
        else:
            while(j<n):
                if nums[j]!=nums[i-1]:
                    nums[i]=nums[j]
                    i=i+1
                j=j+1

        return i
